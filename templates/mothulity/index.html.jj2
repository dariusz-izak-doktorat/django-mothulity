{% extends "mothulity/base.html.jj2" %}
{% block title %}
  Mothulity Index
{% endblock %}
{% block content %}
  <article class="w3-container w3-margin w3-card-8 w3-white">
    <h1>Hello from mothulity index!</h1>
    <h2>I say: {{ msg }}</h2>
  </article>
  <article class="w3-container w3-margin w3-card-8 w3-white">
    <h2>Files upload</h2>
    <section>
      <form method="post" enctype="multipart/form-data">
        {% csrf_token %}
        {{ form.as_p }}
        <p><button class="w3-button w3-black" type="submit">Submit</button></p>
      </form>
    </section>
  </article>
{% endblock %}
